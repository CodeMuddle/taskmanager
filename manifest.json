{
    "manifest_version": 2,
    "name": "Task Manager",
    "description": "Task Manager",
    "version": "0.1.0.0",
    "background": {
        "scripts": [
            "event.js"
        ],
        "persistent": true
    },
    "permissions": [
        "background",
        "tabs", "contextMenus", "storage", "notifications"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}