{
    "manifest_version": 2,
    "name": "Productivity Matrix",
    "description": "Comprises of Eisenhower Matrix. And simple note app",
    "version": "0.1.1.0",
    "background": {
        "scripts": [
            "event.js"
        ],
        "persistent": true
    },
    "permissions": [
        "background", "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}